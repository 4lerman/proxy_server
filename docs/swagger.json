    "host": "localhost:8080",
    "basePath": "/api",
    "paths": {
        "/proxy": {
            "post": {
                "description": "Proxies a request to a third-party service and returns the response",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "proxy"
                ],
                "summary": "Proxy request to a third-party service",
                "parameters": [
                    {
                        "description": "Proxy Request",
                        "name": "proxyRequest",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/models.ProxyRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/models.ProxyResponse"
                        }
                    },
                    "400": {
                        "description": "Invalid request body",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "Internal server error",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "models.ProxyRequest": {
            "type": "object",
            "properties": {
                "headers": {
                    "type": "object",
                    "additionalProperties": {
                        "type": "string"
                    },
                    "example": {
                        "Authorization": "Basic bG9naW46cGFzc3dvcmQ="
                    }
                },
                "method": {
                    "type": "string",
                    "example": "GET"
                },
                "url": {
                    "type": "string",
                    "example": "http://google.com"
                }
            }
        },
        "models.ProxyResponse": {
            "type": "object",
            "properties": {
                "headers": {
                    "type": "object",
                    "additionalProperties": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    }
                },
                "id": {
                    "type": "string"
                },
                "method": {
                    "type": "integer"
                },
                "status": {
                    "type": "integer"
                }
            }
        }
    }
